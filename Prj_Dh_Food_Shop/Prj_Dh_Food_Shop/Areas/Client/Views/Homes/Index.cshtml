    <script type="text/javascript">
        function loadJsAsync(src, callback) {
            var s = document.createElement('script');
            s.type = "text/javascript";
            s.src = src;
            s.addEventListener('load', function (e) { callback(null, e); }, false);
            var head = document.getElementsByTagName('head')[0];
            head.appendChild(s);
        }

        window.addEventListener("load", function () {
            try {
                var enable = <%=ConfigurationManager.AppSettings["TurnOnIPCCChatFrame"] %>;
                var mobileScreenWidth = screen.width;
                if (enable.toString().toLowerCase() == "true" && mobileScreenWidth > 480) {
                    loadJsAsync('https://econtact.viettel.vn:8909/js/IpccChat.js', function () {
                        var IpccChat = {
                            domain: 'V_TRACKING',
                            username: '<%= CurrentSession.CurrentUser.username %>',
                            color: ''
                        };
                        embedIpccChat(IpccChat);
                    })
                }
            }
            catch (err) {
                //do nothing - duongpx2
            }
        }, false);
    </script>
