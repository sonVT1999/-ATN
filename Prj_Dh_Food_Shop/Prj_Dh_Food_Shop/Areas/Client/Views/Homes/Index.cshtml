    <script type="text/javascript">
        function ValidateLogin() {
            var i = $("#txtUser").val();
            var t = $("#txtPass").val();
            var o = $("form").attr("data-message");
            if (i == "") {
                SubmitClick("Bạn chưa nhập tên đăng nhập!", "warning");
                $("#txtUser").focus();
            } else if (t == "") {
                SubmitClick("Bạn chưa nhập mật khẩu!", "warning");
                $("#txtPass").focus();
            } else {
                $(".item-loadding").css("display", "block");
                $("body").addClass("scroll-loadding");
                var a = $.magnificPopup.instance;
                a.close();
                return true;
            }
            return false;
        }

        function SubmitClick(e, n) {
            var i = "listMessage fadeInDown animated";
            if (n == "success") {
                i = i + " success"
            } else if (n == "warning") {
                i = i + " warning"
            } else {
                i = i + " error"
            }
            $("#listMessage").append(" <li class='" + i + "'><span>" + e + "</span></li>");
            setTimeout(function () {
                $("#listMessage li:first-child").remove()
            }, 3e3);
        }
    </script>
    <script type="text/javascript">
        window.onscroll = function () { myFunction() };

        var header = document.getElementById("myHeader");
        var sticky = header.offsetTop;

        function myFunction() {
            if (window.pageYOffset > sticky) {
                header.classList.add("sticky");
            } else {
                header.classList.remove("sticky");
            }
        }
    </script>
